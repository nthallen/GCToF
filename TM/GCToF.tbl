  ""      (TT1_Start,5);
  Speed:  (TT1_Speed,4);
  WtrCl:  (TT1_WtrCl,3);
  ActStp: (TT1_ActStp,3);
  Vent:   (TT1_VentVlv,6);
  VentOp: (TT1_VentOp,4);
  I:      (TT1_I,6)    mA;
  V:      (TT1_V,3)    V;
  W:      (TT1_W,3)    W;
  T:      (TT1_T,3)    C;
  Drive:  (TT1_drvf,5) Hz;
  Speed:  (TT1_spd,5)  Hz;
  Status: (TT1_Status,8);
